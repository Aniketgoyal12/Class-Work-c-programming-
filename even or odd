#include <stdio.h>

//Program to check if number is even or odd//

int main()

{
    int a;
    printf("Enter the number");
    scanf("%d",&a);
    
    if(a / 2 == 0)
    {
        printf("Given number is even");
        
    }
    
    else
    {
        printf("Given number is odd");
    }
    
    return 0;
}
